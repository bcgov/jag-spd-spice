{
    "kind": "List",
    "apiVersion": "v1",
    "metadata": {},
    "items": [
        {
            "apiVersion": "image.openshift.io/v1",
            "kind": "ImageStream",
            "metadata": {
                "name": "spice-carla-sync-service"
            }
        },
        {
            "apiVersion": "build.openshift.io/v1",
            "kind": "BuildConfig",
            "metadata": {
                "labels": {
                    "app": "spice-carla-sync-service"
                },
                "name": "spice-carla-sync-service"
            },
            "spec": {
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "spice-carla-sync-service:latest"
                    }
                },
                "resources": {
                    "limits": {
                        "cpu": "1",
                        "memory": "4Gi"
                    },
                    "requests": {
                        "cpu": "1",
                        "memory": "3Gi"
                    }
                },
                "runPolicy": "Serial",
                "source": {
                    "contextDir": "",
                    "git": {
                        "ref": "develop",
                        "uri": "https://github.com/bcgov/jag-spd-spice.git"
                    },
                    "type": "Git"
                },
                "strategy": {
                    "sourceStrategy": {
                        "env": [
                            {
                                "name": "DOTNET_STARTUP_PROJECT",
                                "value": "carla-sync-service/CarlaSync.csproj"
                            }
                        ],
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "dotnet-22-rhel7:latest"
                        }
                    },
                    "type": "Source"
                },
                "triggers": [
                    {
                        "type": "ImageChange"
                    },
                    {
                        "type": "ConfigChange"
                    }
                ]
            }
        }
    ]
}
